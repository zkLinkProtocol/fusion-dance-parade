{
  "name": "boilerplate-nextjs",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "stylelint --allow-empty-input \"**/*.{css,scss}\" && next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "postinstall": "husky install",
    "test:unit": "vitest run",
    "watch:unit": "vitest watch",
    "coverage": "vitest run --coverage",
    "coverage:preview": "serve coverage -l 3001",
    "prepare": "is-ci || husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:preview": "serve storybook-static -l 3001",
    "generate:contracts-types": "rimraf src/types/contracts && typechain --target=ethers-v5 --out-dir=src/types/contracts 'src/constants/contracts/abis/*.json'"
  },
